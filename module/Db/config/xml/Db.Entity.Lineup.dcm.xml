<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\Lineup">
    <id name="id" type="integer"/>
    <field name="name" type="string" nullable="false"/>
    <field name="note" nullable="true"/>
    <many-to-one field="band" target-entity="Db\Entity\Band" inversed-by="lineups">
      <join-columns>
        <join-column name="Band_id" referenced-column-name="id" nullable="false"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="shows" target-entity="Db\Entity\Show" mapped-by="lineup"/>
    <many-to-many field="performers" target-entity="Db\Entity\Performer" mapped-by="lineups"/>
  </entity>
</doctrine-mapping>
