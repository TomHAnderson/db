<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Db\Entity\Zipcode">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="zipcode" type="string" unique="true" nullable="false"/>
    <one-to-many field="venues" target-entity="Db\Entity\Venue" mapped-by="zipcode"/>
    <one-to-many field="events" target-entity="Db\Entity\Event" mapped-by="zipcode"/>
    <many-to-many field="cities" target-entity="Db\Entity\City" mapped-by="zipcodes"/>
  </entity>
</doctrine-mapping>
