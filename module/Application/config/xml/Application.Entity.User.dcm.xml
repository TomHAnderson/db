<?xml version="1.0"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xsi="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\User">
    <id name="id" type="integer">
      <generator strategy="AUTO"/>
    </id>
    <field name="username" type="string" nullable="true"/>
    <field name="email" type="string" nullable="true"/>
    <field name="password" type="string" nullable="true"/>
    <field name="displayName" type="string" nullable="true"/>
    <field name="accessToken" type="string" nullable="true"/>
    <field name="createdAt" type="datetime" nullable="true"/>
    <field name="lastRequestAt" type="datetime" nullable="true"/>
    <field name="note" type="text" nullable="true"/>
    <field name="isPublic" type="boolean" nullable="true"/>
    <field name="perms" type="string" nullable="true"/>
  </entity>
</doctrine-mapping>
